{
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "name": "Repost Ad",
  "version": "1.0",
  "short_name": "Repost ad",
  "content_scripts": [
  {
    "matches": [
      "https://www.pinkbike.com/*"
    ],
    "js": ["content.js"]
  }
],
"browser_action": {
  "default_icon": "icon.png",
    "default_popup":"popup.html"
},
"permissions": [
    "tabs"
  ]
}
